package com.dsa;


public class LinearSearch {
	
	public static int linearSearch(int arr[], int target) {
		int result = 0;
		for(int i = 0; i < arr.length; i++) {
			if(arr[i] == target) {
				result = arr[i];
				break;
			}
		}
		
		return result;
		
	}
	
	public static void main(String[] args) {
		
			
				
	}

}
