package com.dsa;

public class PrintElementAlternative {
	
	public static int[] printElementAlternativ(int[] arr) {
		int newArray[] = new int[arr.length];
		for(int i = 0; i < arr.length; i++) {
			if(i % 2 == 0) {
				newArray = arr[i];
			}
		}
	}
	
	public static void main(String[] args) {
		
	}

}
