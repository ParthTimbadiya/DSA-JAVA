package com.dsa;

public class MergeTwoSortedArray {
	
	public static int[] mergeTwoSortedArray(int[][] arr) {
		
		int a[] = arr[0];
		int b[] = arr[1];
		int merge[] = new int[a.length + b.length];
		
		return merge;
	}
	
	public static void main(String[] args) {
		
		int[][] arr = {{1,3,5},{2,4,6}};
		
	}

}
